fn print_number(n int) {
    if n == 0 { println('0') }
    if n == 1 { println('1') }
    if n == 2 { println('2') }
    if n == 3 { println('3') }
    if n == 4 { println('4') }
    if n == 5 { println('5') }
    if n == 6 { println('6') }
    if n == 7 { println('7') }
    if n == 8 { println('8') }
    if n == 9 { println('9') }
}

fn test_add() {
	println('test_add()')
	x := 2
	y := 3
	z := x + y
	q := y - x
	print_number(x)
	print_number(y)
	print_number(z)
	print_number(q)
}

fn main() {
	println('start')
	test_add()
	println('end')
}
